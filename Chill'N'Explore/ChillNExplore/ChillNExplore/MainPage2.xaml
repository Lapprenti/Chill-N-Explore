             x:Class="ChillNExplore.MainPage2"
             BackgroundColor="#FF72E68C"
         >

    <StackLayout Padding="10" HorizontalOptions="Center" VerticalOptions="Center" BackgroundColor="#FF72E68C">
        <Image x:Name="ParametreLogo" Source="Param.png"></Image>
